{
    "version": 2,
    "builds": [
      {
        "src": "mypro/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "mypro/wsgi.py"
      }
    ],
    "build": {
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "commands": [
        "pip install -r requirements.txt",
        "python3 manage.py collectstatic --noinput"
      ]
    }
  }
  